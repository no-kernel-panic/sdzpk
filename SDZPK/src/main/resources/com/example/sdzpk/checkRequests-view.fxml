      <Button fx:id="rejectButton" layoutX="82.0" layoutY="298.0" mnemonicParsing="false" text="Reject" visible="false" />
      <Button fx:id="confirmButton" layoutX="206.0" layoutY="298.0" mnemonicParsing="false" onAction="#confirmButtonAction" text="Confirm" visible="false" />
